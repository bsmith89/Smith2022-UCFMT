@article{Kump2018,
  title        = {The taxonomic composition of the donor intestinal
    microbiota is a major factor influencing the efficacy of faecal
    microbiota transplantation in therapy refractory ulcerative
    colitis},
  author       = {Kump, P. and Wurm, P. and Gr\"ochenig, H. P. and
    Wenzl, H. and Petritsch, W. and Halwachs, B. and Wagner, M. and
